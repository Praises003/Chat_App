{
    "version": 2,
    "name": "event",
    "builds": [
      {
        "src": "backend/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "backend/index.js",
        "methods": ["GET", "POST","PUT", "DELETE", "OPTIONS"],
        "headers": {
            "Access-Control-Allow-Origin": "https://chat-app-frontend-b4kq.onrender.com/"
        }
      }
    ]
    
  }
  